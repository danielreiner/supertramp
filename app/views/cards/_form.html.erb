<%= simple_form_for(theme_card_path(@theme, @card)) do |f| %>
  
  <%= f.input :title, :label => "Themenname", :input_html => { :class => "span4" } %>
  <%= f.input :user_id, :label => "Autor" %>
  <%= f.input :theme_id, :label => "Theme_id" %>
  <%= f.submit "Karte anlegen", :class => "primary" %>
<% end %>

